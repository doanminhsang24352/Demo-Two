<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Tag:</strong>
    <%= product.tag %>
  </p>

  <p>
    <strong>String:</strong>
    <%= product.string %>
  </p>

</div>
